<%- include('header'); -%>

  <title>Employee List</title>
</head>
<body>
  <h1>Employee List</h1>
  <table>
    <tr>
     
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Age</th>
      <th>Department</th>
      <th>Salary</th>

    </tr>
    <% employees.forEach(employee => { %>
    <tr>
     
      <td><%= employee.fname %></td>
      <td><%= employee.lname %></td>
      <td><%= employee.email %></td>
      <td><%= employee.age %></td>
      <td><%= employee.department %></td>
      <td><%= employee.salary %></td>

    </tr>
    <% }) %>
  </table>


<%- include('footer'); -%>